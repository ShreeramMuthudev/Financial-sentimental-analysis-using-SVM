        "df_neigh_radius= pd.read_csv('/content/NeighRadius.csv')\n",
        "df_radius = pd.read_csv('/content/Radius.csv')"
      ],
      "metadata": {
        "id": "LpfdQpdT4RwD"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.stats import pearsonr\n",
        "import statsmodels.api as sm"
      ],
      "metadata": {
        "id": "fWHWp-1f4W41"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import statsmodels.api as sm\n",
        "\n",
        "# Assuming your DataFrames (df_face, df_neigh_face, df_neigh_radius, df_radius) are already loaded as shown\n",
        "\n",
        "# Initialize a DataFrame to collect summary statistics for each time step\n",
        "summary_statistics = pd.DataFrame()\n",
        "\n",
        "# Loop through each time step (assuming time steps are named from 't1' to 't1882')\n",
        "for time_step in range(1, 1883):  # Adjust the range according to your actual number of time steps\n",
        "    time_point = f't{time_step}'  # Construct the column name for the current time step\n",
        "\n",
        "    # Check if the time_point exists in all DataFrames to handle datasets with varying time steps\n",
        "    if time_point in df_radius.columns and time_point in df_face.columns and \\\n",
        "       time_point in df_neigh_radius.columns and time_point in df_neigh_face.columns:\n",
        "\n",
        "        # Concatenate data for the current time step from all datasets\n",
        "        data_at_time_point = pd.concat([\n",
        "            df_radius[[time_point]].rename(columns={time_point: 'Radius'}),\n",
        "            df_face[[time_point]].rename(columns={time_point: 'Face'}),\n",
        "            df_neigh_radius[[time_point]].rename(columns={time_point: 'NeighRadius'}),\n",
        "            df_neigh_face[[time_point]].rename(columns={time_point: 'NeighFace'})\n",
        "        ], axis=1)\n",
        "\n",
        "        # Prepare the independent (X) and dependent variables (y) for regression\n",
        "        X = data_at_time_point[['Face', 'NeighRadius', 'NeighFace']]\n",
        "        y = data_at_time_point['Radius']\n",
        "\n",
        "        # Add a constant to the model for the intercept\n",
        "        X = sm.add_constant(X, has_constant='add')\n",
        "\n",
        "        # Fit the OLS regression model\n",
        "        model = sm.OLS(y, X, missing='drop').fit()  # 'missing='drop'' automatically excludes NaNs\n",
        "\n",
        "        # Extract and store the model's summary statistics\n",
        "        summary_statistics = summary_statistics.append({\n",
        "            'Time Step': time_point,\n",
        "            'R-squared': model.rsquared,\n",
        "            'Coefficients': model.params\n",
        "        }, ignore_index=True)\n",
        "\n",
        "# Display the summary statistics for the first few time steps\n",
        "print(summary_statistics.head())\n"
      ],
      "metadata": {
